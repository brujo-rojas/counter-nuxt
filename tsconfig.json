{
  // https://nuxt.com/docs/guide/concepts/typescript
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~/*": ["./*"]
    }
  },
  "include": [
    "./components/**/*.ts",
    "./components/**/*.tsx",
    "./components/**/*.vue",
    "./composables/**/*.ts",
    "./layouts/**/*.ts",
    "./middleware/**/*.ts",
    "./pages/**/*.ts",
    "./plugins/**/*.ts",
    "./server/**/*.ts",
    "./stores/**/*.ts",
    "./types/**/*.ts"
  ]
}
